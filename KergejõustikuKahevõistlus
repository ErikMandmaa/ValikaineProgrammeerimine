def arvuta_jooksu_punktid(aeg):
    A = 25.4347
    B = 18
    C = 1.81
    punktid = A * (B - aeg) ** C
    return round(punktid)


def arvuta_kaugushüppe_punktid(kaugus):
    A = 0.14354
    B = 220
    C = 1.4
    punktid = A * (kaugus - B) ** C
    return round(punktid)


jooksu_aeg = float(input("Sisesta 100 meetri jooksu aeg sekundites (kaks koma kohta): "))
kaugushüppe_kaugus = int(input("Sisesta kaugushüppe kaugus sentimeetrites (täisarv): "))


jooksu_punktid = arvuta_jooksu_punktid(jooksu_aeg)
kaugushüppe_punktid = arvuta_kaugushüppe_punktid(kaugushüppe_kaugus)


kokku = jooksu_punktid + kaugushüppe_punktid
print(f"100 meetri jooksu punktid: {jooksu_punktid - 1}")
print(f"Kaugushüppe punktid: {kaugushüppe_punktid - 1}")
print(f"Kokku saadud punktid: {kokku - 1}")
print(f"jooksu_aeg = 2013")
    